<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.k2.processmining.mapper.testMapper">

    <resultMap id="testPojo" type="org.k2.processmining.model.testPojo">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="username" property="username" jdbcType="VARCHAR"/>
    </resultMap>

    <insert id="add" parameterType="org.k2.processmining.model.testPojo" >
        INSERT INTO testpojo VALUES (#{id,jdbcType = INTEGER},#{username,jdbcType = VARCHAR})
    </insert>

    <select id="showAll" resultMap="testPojo">
        SELECT * FROM testPojo
    </select>

</mapper>